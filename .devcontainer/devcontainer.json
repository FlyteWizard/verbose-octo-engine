{
   "id": "uvic-quantum-computing",
  "name": "UVic Quantum Computing",
  "description": "",
  "documentationURL": "",
  "remoteUser": "root",
  "image": "mcr.microsoft.com/quantum/iqsharp-base:latest",
  "customizations": {
    "vscode": {
      "extensions": [
        "GitHub.codespaces",
        "GitHub.github-vscode-theme",
        "ms-python.isort",
        "ms-python.python",
        "ms-python.vscode-pylance",
        "ms-toolsai.jupyter-keymap",
        "ms-toolsai.jupyter-renderers",
        "ms-toolsai.jupyter",
        "ms-toolsai.vscode-jupyter-cell-tags",
        "ms-toolsai.vscode-jupyter-slideshow",
        "ms-vscode-remote.remote-containers",
        "ms-vscode.csharp",
        "quantum.quantum-devkit-vscode"
      ]
    }
  },
  "features": {
    "ghcr.io/devcontainers/features/github-cli:latest": {},
    "ghcr.io/devcontainers-contrib/features/zsh-plugins:latest": {},
    "ghcr.io/meaningful-ooo/devcontainer-features/homebrew:latest": {},
    "ghcr.io/rocker-org/devcontainer-features/pandoc:latest": {},
    "ghcr.io/rocker-org/devcontainer-features/apt-packages:latest": {
      "packages": "texlive-xetex,texlive-fonts-recommended,texlive-plain-generic"
    }
  },
  "postCreateCommand": "pip install -r .devcontainer/requirements.txt"
}
